<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          xmlns:tools="http://schemas.android.com/tools"
          package="gdl.dreamteam.skynet">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="WatchDogs"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppThemeLogIn">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ClientsActivity" android:theme="@style/AppTheme"></activity>
        <activity android:name=".Activities.DeviceActivity" android:theme="@style/AppTheme"></activity>
        <activity android:name=".Activities.ZonesActivity" android:theme="@style/AppTheme"></activity>

    </application>

</manifest>